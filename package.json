    "@types/mocha": "2.2.46",
    "@types/sequelize": "^4.27.1",
    "@types/sinon": "4.1.2",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-typescript": "3.2.3",
    "merge2": "1.2.1",
    "mocha": "4.1.0",
    "nyc": "11.4.1",
    "pg": "^6.4.2",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.31.0",
    "should": "13.2.0",
    "sinon": "4.1.4",
