  },
  "license": "MIT",
  "author": {
    "name": "Vadim Petrov"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CodeZavod/pg-scheduler.git"
  },
  "engines": {
    "node": ">=8.4.0"
  },
  "files": [
    "dst/"
  ],
  "main": "dst/Scheduler.js",
  "typings": "dst/Scheduler.d.ts",
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
